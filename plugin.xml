      <primaryWizard id="org.eclipse.uide.wizards.NewLanguageWizard"/>
      <category
            id="org.eclipse.uide.wizards"
            name="Universal IDE"/>
      <wizard
            class="org.org.eclipse.uideeclipse.uide.wizards.Wizards$NewLanguage"
